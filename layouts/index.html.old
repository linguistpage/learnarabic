{{ define "main" }}
{{ .Content }}
{{ partial "menu.html" . }}
{{ partial "homesearch.html" . }}
{{ partial "table.html" . }}
<!--######## TAGS #########-->
<div style="padding:10px;" class="tag-cloud">
{{ if isset .Site.Taxonomies "tags" }}
{{ if not (eq (len .Site.Taxonomies.tags) 0) }}
<h3 style="margin-bottom: 10px;">Tags </h3>
{{ range $name, $items := .Site.Taxonomies.tags }}
{{ $len := len $items }}
<small style="display: inline-block;">
  <a id="searchable" href="{{ $.Site.BaseURL }}/tags/{{ $name | urlize | lower }}">
    {{ $name | title }} ({{ len $items }}) </a> &#160 </small>
{{ end }}
{{ end }}
{{ end }}
</div>
{{ end }}
